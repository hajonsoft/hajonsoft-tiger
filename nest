firebase login
rm .firebaserc
firebase use --add $1
git checkout master
firebase auth:export --format json users
read -t 5
cat users
firebase apps:sdkconfig > $1.js
firebase init hosting:github < nest-input
read -t 30 
mv $1.js src/firebaseConfigs
cp .github/workflows/template .github/workflows/$1.yml
node nest-script $1
read -t 20 
firebase deploy --only database
read -t 30
firebase deploy --only storage
rm .github/workflows/firebase-hosting-pull-request.yml
# npm run build
# firebase deploy --only hosting
git restore src/firebaseConfig.js
git add .
git commit -m $1
git show HEAD